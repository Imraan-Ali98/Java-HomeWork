public class SingleRotation
{

void rough()
{
int a[]= {4,5,6,7,8,9};
int temp=a[0];

for(int i=0;i<a.length-1;i++)
 {
  a[i]=a[i+1];
 }
a[a.length-1]=temp;

for(int k: a)
{System.out.print(k+" ");}
}

public static void main(String args[])
{
SingleRotation obj= new SingleRotation();
obj.rough();
}
}
