public class Rough
{

void rough()
{
int a[]= {7,5,5,1,3};

for(int i=0;i<a.length;i++)
{
 for(int j=i+1;j<a.length;j++)
  {
   if(a[i]==a[j])
   {a[j]=0;} // Making duplicates as zero
  }
}
for(int k: a)
{System.out.print(k+" ");}

}

public static void main(String args[])
{
Rough obj= new Rough();
obj.rough();
}
}
