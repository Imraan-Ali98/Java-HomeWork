package numerics;

public class ReversingNumber 
{
	static void reverse()
	{
	int num1= 123456789;
	int num=num1;
	int n=0, counter=0;
		
		while(num!=0)
		{
		n= num % 10;
		num= num/10;
		counter++;	
		}
	int arr[]= new int[counter];
	for(int i=0;i<arr.length;i++)
	{
		arr[i]= num1 % 10;
		num1= num1/10;
		//System.out.print(arr[i]);
	}
		for(int k: arr)
		{
		System.out.print(k+" ");
		}
	}
	public static void main(String[] args) 
	{
		reverse();
	}
}
